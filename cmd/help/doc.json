{
    "command": "doc",
    "desc": "Generate documentation.",
    "usage": [
        "doc [<options>]",
        "doc util.js -o docs.html"
    ],
    "options": [
        {
            "shorthand": "-d",
            "flag": "--description",
            "description": "Extra description markdown file path."
        },
        {
            "shorthand": "-o",
            "flag": "--output",
            "description": "Output path, default to docs.html"
        },
        {
            "shorthand": "-t",
            "flag": "--title",
            "description": "Documentation title"
        },
        {
            "flag": "--format",
            "description": "Output format, html, json or markdown, defaults to html"
        }
    ]
}