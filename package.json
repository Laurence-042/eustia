{
  "name": "eustia",
  "version": "0.3.6",
  "description": "Tool for generating utility libraries",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eustia.git"
  },
  "bin": {
    "eustia": "./bin/eustia.js"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint lib cmd bin",
    "updateEris": "node script/updateEris",
    "cov": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly"
  },
  "eustia": {
    "output": "lib/util.js",
    "files": ["index.js", "bin/eustia.js", "cmd/**/*.js", "lib/*.js"],
    "format": "commonjs"
  },
  "keywords": [
    "utility",
    "library",
    "custom",
    "javascript",
    "underscore"
  ],
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eustia/issues"
  },
  "homepage": "https://github.com/liriliri/eustia#readme",
  "dependencies": {
    "async": "^1.4.2",
    "chalk": "^1.1.1",
    "chokidar": "^1.4.2",
    "glob": "^6.0.1",
    "handlebars": "^4.0.3",
    "marked": "^0.3.5",
    "nopt": "^3.0.4",
    "qs": "^6.0.2",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  }
}
